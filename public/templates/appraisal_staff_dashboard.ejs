<div class="container" >


    <div class="row padding">
        <div class="col-md-4" ng-repeat="d in infor.data.result">
            <div class="panel panel-default">

                <div class="panel-heading">

                    <h2 class="panel-title" >{{ d.subject_name }}</h2>
                    <h3 class="col-lg-12" > {{ d.type }}</h3>

                </div>
                <div class="panel-body">
                    <h4>Class: <strong>{{ d.code }}</strong></h4>
                    <h4>Student Strength: <strong>{{ d.total }}</strong>
                        <h4>Students Appraised: <strong>{{ d.appraised }}</strong>
                            <br><br>
                            
                            <button class="btn btn-primary" role="button" ng-click = 'viewReport(d.code,d.table)' >View Apprisal</button>
                </div>
            </div>
        </div>
        <div class="col-md-4">
        </div>

    </div>


