    <div class="container boxed">
        <div class="row">
            <div class="col-xs-12" style="position:relative;">
                <div class="inner">
                    <img src="/images/logo.jpg" class="logorv">
                </div>
                <div class="col-xs-12 inner margin-b">
                    <h1 class="rvce text-center"><strong>RV College of Engineering</strong></h1>
                    <h4 class="text-center padding-bott">(Autonomous Institute under VTU)</h4>
                    <h1 class="rvce text-center rvce2">{{ stype }} Course - Appraisal Sheet</h1>
                    <h4 class="text-center"><strong>Jan-15 / Apr-15</strong></h4>
                </div>
            </div>
            
        </div>
        <div class="gap"></div>
        <div class="row" style="margin-bottom: 20px;" >
            <div class="col-xs-6">
                <h3>Name: <strong>{{ teacher }}</strong></h3>
                <h3>Dept: <strong>{{ department }}</strong></h3>
                <h3>Subject: <strong>{{ subjectname }}</strong> </h3>
                <h3>No. of students: <strong>{{  total }}</strong></h3>
            </div>
            <div class="col-xs-6">
                <h3>Designation: <strong>{{ designation }}</strong></h3>
                <h3>Semester: <strong>4</strong></h3>
                <h3>Class(Section): <strong>C</strong></h3>
                <h3>No of students who have appraised: <strong>{{ done }}</strong></h3>
            </div>
            
        </div>

        <table class="table table-hover table-report table-bordered">
           <thead>
              <tr>
                 <th>Q.No.</th>
                 <th>Question</th>
                 <th>Score</th>
                 <th>Percentage</th>
              </tr>
           </thead>
           <tbody>
              <tr  ng-repeat="(key,value) in question">
                 <td class="col-md-1">{{ $index+1 }}</td> 
                 <td class="col-md-9">
                    {{key}}
                 </td>
                 
                 <td class="col-md-1">{{value}}</td>
                 <td class="col-md-1">{{value/done * 10}}</td>
              </tr>

             


           </tbody>
        </table>



        <div class="row">
            <div class="col-xs-12">
                <h3> <strong>Comments: </strong></h3>

                <!--<p class="table-report">Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua.</p>-->
            </div>
        </div>

    </div>